<!doctype html>
<html lang="zh-CN">
<head>
    <meta charset="utf-8">
    <title>{$mad_configs.title}</title>
    <meta name="keywords" content="{$mad_configs.keywords}" />
    <meta name="description" content="{$mad_configs.description}" />
    <link rel="stylesheet" href="{$view_path}/css/style.css">
    <script type="text/javascript" src="{$view_path}/js/jquery-1.9.0.js"></script>
    <script type="text/javascript" src="{$view_path}/js/jquery.easing.1.3.js"></script>
</head>
<body>
	{include file='header.mad'}
	{include file='user_body.mad'}
	{if $act eq 'index'}
	{include file='user_index.mad'}
	{else if $act eq 'userinfo'}
	{include file='user_info.mad'}
	{else if $act eq 'prepayment'}
	{include file='user_prepayment.mad'}
	{else if $act eq 'encashment'}
	{include file='user_encashment.mad'}
	{else if $act eq 'vipmsg'}
	{include file='user_vipmsg.mad'}
	{else if $act eq 'primary'}
	{include file='user_spreader.mad'}
	{else if $act eq 'vip_article'}
	{include file='user_vip_article.mad'}
	{else if $act eq 'vip_article_id'}
	{include file='user_vip_article_id.mad'}
	{/if}
	{include file='footer.mad'}

</body>
</html>
